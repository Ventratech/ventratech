{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid"
    },
    "description": {
      "type": "text"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "custom_builds",
        "upgrade_bundles",
        "components",
        "networking",
        "monitors",
        "peripherals",
        "wireless_internet"
      ]
    }
  }
}
